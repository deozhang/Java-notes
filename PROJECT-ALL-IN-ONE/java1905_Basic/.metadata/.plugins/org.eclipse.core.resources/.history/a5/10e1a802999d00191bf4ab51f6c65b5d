package com.xms.se.day05;

import java.util.HashMap;
import java.util.Map;

public class Demo06Map {
	public static void main(String[] args) {
		Map<String,Integer> map = new HashMap<String, Integer>();
		map.put("语文", 88);
		map.put("数学", 99);
		map.put("英语", 88);
		map.put("物理", 77);
		map.put("化学", 66);
		map.put("生物", 55);	
//		遍历所有的key
		
		
	}

}
